<?xml version="1.0" encoding="UTF-8"?>
<web-app>
	<servlet>
		<servlet-name>forward</servlet-name>
		<servlet-class>org.irssi.webssi.server.ForwardServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>forward</servlet-name>
		<url-pattern>/org.irssi.webssi.Webssi/events.json</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>forward</servlet-name>
		<url-pattern>/org.irssi.webssi.TestWebssi/events.json</url-pattern>
	</servlet-mapping>
</web-app>
